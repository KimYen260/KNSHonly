<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../partials/header'); %>
</header>

<main>
    <div class="row">
        <div class="col-sm-5">
          <div class="jumbotron">
            <h1>This is EJS tags</h1>
            <p>Welcome to understanding EJS tags</p>
          </div>
        </div>
      
        <div class="col-sm-7">
          <div class="well">
            <h2>Look I'm an assistant, <%- pet.petName %>!</h2>
          </div><br>
          <div>
            <div><% tags.forEach(function(obj) { %>
                <h3><%= obj.order %></h3>
                <h4><strong><%= obj.tagIcon %></strong>
                meaning <%= obj.Name %><br>
                <% }); %></h4>
            </div>
          </div>
        </div>
      </div>
</main>

<footer>
    <%- include('../partials/footer'); %>
  </footer>
  
  
  
  </body>
  </html>
  